<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settingsVM" type="com.application.app.modules.settings.data.viewmodel.SettingsVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSettings"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imagePattern"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_pattern"
            tools:ignore="ContentDescription" tools:src="@drawable/img_pattern"/>
        <TextView android:ellipsize="end" android:id="@+id/txtSeven"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_32pxv"
            android:layout_marginEnd="@dimen/_107pxh"
            android:layout_marginStart="@dimen/_107pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_5pxh"
            android:singleLine="true"
            android:text="@{settingsVM.settingsModel.txtSeven}"
            style="@style/txtSenregular24" tools:text="@string/lbl13"/>
    </FrameLayout>
</layout>
